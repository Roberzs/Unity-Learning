<?xml version="1.0" encoding="utf-8"?>
<data name = "MonsterData" from = "GMonster.xlsx" to = "MonsterData.xml">
	<variable name = "AllMonster" type = "list">
		<list name = "MonsterBase" sheetname = "怪物配置表" mainKey = "Id">
			<variable name = "Id" col = "ID" type = "int"/>
			<variable name = "Name" col = "名字" type = "string"/>
			<variable name = "OutLook" col = "路径" type = "string"/>
			<variable name = "Level" col = "等级" type = "int"/>
			<variable name = "Rare" col = "稀有度" type = "int"/>
			<variable name = "Height" col = "高度" type = "float"/>
		</list>
	</variable>
</data>